<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="piano"
          class="norbsoft.sia.part1.domain.impl.Piano" />

    <bean id="saxophone"
          class="norbsoft.sia.part1.domain.impl.Saxophone" />

    <bean id="kenny"
          class="norbsoft.sia.part1.domain.impl.Instrumentalist"
          p:instrument-ref="piano"
          p:song="Passed-by song name" />

    <bean id="carl"
          class="norbsoft.sia.part1.domain.impl.Instrumentalist"
          p:instrument-ref="saxophone"
          p:song="#{kenny.song}" />

    <bean id="rocksongs"
          class="norbsoft.sia.part1.domain.impl.RockSongSelector" />

    <bean id="song1"
          class="norbsoft.sia.part1.domain.impl.Instrumentalist"
          p:instrument-ref="piano"
          p:song="#{rocksongs.selectSong()?.toUpperCase()?:'Hmm, null song...'}"/>

    <bean id="song2"
          class="norbsoft.sia.part1.domain.impl.Instrumentalist"
          p:instrument-ref="piano"
          p:song="#{rocksongs.selectSong()?.toUpperCase()?:'Hmm, null song...'}"/>

    <bean id="song3"
          class="norbsoft.sia.part1.domain.impl.Instrumentalist"
          p:instrument-ref="piano"
          p:song="#{rocksongs.selectSong()?.toUpperCase()?:'Hmm, null song...'}"/>

    <bean id="song4"
          class="norbsoft.sia.part1.domain.impl.Instrumentalist"
          p:instrument-ref="piano"
          p:song="#{rocksongs.selectSong()?.toUpperCase()?:'Hmm, null song...'}"/>


    <util:list id="cities">
        <bean class="norbsoft.sia.part1.domain.impl.City"
              p:name="Chicago" p:state="IL" p:population="2853114" />
        <bean class="com.habuma.spel.cities.City"
              p:name="Atlanta" p:state="GA" p:population="537958"/>
        <bean class="com.habuma.spel.cities.City"
              p:name="Dallas" p:state="TX" p:population="1279910"/>
        <bean class="com.habuma.spel.cities.City"
              p:name="Houston" p:state="TX" p:population="2242193"/>
        <bean class="com.habuma.spel.cities.City"
              p:name="Odessa" p:state="TX" p:population="90943"/>
        <bean class="com.habuma.spel.cities.City"
              p:name="El Paso" p:state="TX" p:population="613190"/>
        <bean class="com.habuma.spel.cities.City"
              p:name="Jal" p:state="NM" p:population="1996"/>
        <bean class="com.habuma.spel.cities.City"
              p:name="Las Cruces" p:state="NM" p:population="91865"/>
    </util:list>



</beans>